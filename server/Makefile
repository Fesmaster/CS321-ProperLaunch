# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  ProperLaunchServer_config = debug

else ifeq ($(config),release)
  ProperLaunchServer_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := ProperLaunchServer

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

ProperLaunchServer:
ifneq (,$(ProperLaunchServer_config))
	@echo "==== Building ProperLaunchServer ($(ProperLaunchServer_config)) ===="
	@${MAKE} --no-print-directory -C . -f ProperLaunchServer.make config=$(ProperLaunchServer_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f ProperLaunchServer.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   ProperLaunchServer"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"